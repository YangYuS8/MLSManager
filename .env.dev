# ML-Server-Manager Development Environment Configuration

# Application
APP_NAME=ML-Server-Manager
APP_VERSION=0.1.0
DEBUG=true
ENVIRONMENT=development

# Server
HOST=0.0.0.0
PORT=8000

# Security
SECRET_KEY=dev-secret-key-not-for-production
ACCESS_TOKEN_EXPIRE_MINUTES=1440
ALGORITHM=HS256

# CORS - Allow frontend dev server
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000","http://127.0.0.1:5173"]

# Node Configuration
NODE_TYPE=master
NODE_ID=dev-master-001

# Default Admin User
DEFAULT_ADMIN_USERNAME=mlsmanager
DEFAULT_ADMIN_EMAIL=admin@mlsmanager.local
DEFAULT_ADMIN_PASSWORD=mlsmanager_secret
